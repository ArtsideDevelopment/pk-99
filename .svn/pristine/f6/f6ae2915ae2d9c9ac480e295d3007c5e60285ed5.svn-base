(253, 252, 0, 'Консервные ножи', 'konservnye-nozhi', 'catalog/konservnye-nozhi', NULL, 42832203, NULL, NULL, '&lt;table align=&quot;center&quot; border=&quot;0&quot;&gt;\n&lt;tbody&gt;\n&lt;tr&gt;\n&lt;td&gt;&lt;a href=&quot;/uploads/images/categories/transfer/1_70.jpg&quot; class=&quot;pk-gallery&quot; onclick=&quot;&quot;&gt;&lt;img alt=&quot;Консервный-нож-М-в-работе-1+&quot; src=&quot;/uploads/images/categories/transfer/thumb_768098203_5.jpg&quot; style=&quot;border-width: 0;&quot; height=&quot;135&quot; width=&quot;180&quot; /&gt;&lt;/a&gt;&lt;/td&gt;\n&lt;td&gt;&lt;a href=&quot;/uploads/images/categories/transfer/image_270.jpg&quot; class=&quot;pk-gallery&quot; onclick=&quot;&quot;&gt;&lt;img alt=&quot;Консервный-нож-осн+&quot; src=&quot;/uploads/images/categories/transfer/thumb_768098403_5.jpg&quot; style=&quot;border-width: 0;&quot; height=&quot;135&quot; width=&quot;180&quot; /&gt;&lt;/a&gt;&lt;/td&gt;\n&lt;td&gt;&lt;a href=&quot;/uploads/images/categories/transfer/image_269.jpg&quot; class=&quot;pk-gallery&quot; onclick=&quot;&quot;&gt;&lt;img alt=&quot;Консервный-нож-в-работе+&quot; src=&quot;/uploads/images/categories/transfer/thumb_768097603_5.jpg&quot; style=&quot;border-width: 0;&quot; height=&quot;135&quot; width=&quot;180&quot; /&gt;&lt;/a&gt;&lt;/td&gt;\n&lt;/tr&gt;\n&lt;tr&gt;\n&lt;td style=&quot;text-align: center;&quot;&gt;раз...&lt;/td&gt;\n&lt;td style=&quot;text-align: center;&quot;&gt;два...&lt;/td&gt;\n&lt;td style=&quot;text-align: center;&quot;&gt;три... три... три... &lt;br /&gt;...тридцать восемь&lt;/td&gt;\n&lt;/tr&gt;\n&lt;/tbody&gt;\n&lt;/table&gt;\n&lt;p&gt;Подобным ножам, оказывается, &lt;a target=&quot;_blank&quot; href=&quot;http://ru.wikipedia.org/wiki/P-38_(%EA%EE%ED%F1%E5%F0%E2%ED%FB%E9_%ED%EE%E6)%20&quot;&gt;посвящена целая статья в Википедии &gt;&gt;&lt;/a&gt;&lt;br /&gt;&lt;em&gt;&lt;/em&gt;&lt;/p&gt;', NULL, NULL, NULL, NULL, NULL, NULL, NULL, 1, NULL),
